# Configuración de variables de entorno
# Copia este archivo como .env y configura tus valores

# Base de datos PostgreSQL (Railway)
DATABASE_URL=postgresql://usuario:password@host:puerto/database

# JWT Secrets (genera tus propios valores seguros)
JWT_SECRET=tu_jwt_secret_muy_seguro_aqui_minimo_32_caracteres
REFRESH_JWT_SECRET=tu_refresh_jwt_secret_muy_seguro_aqui_minimo_32_caracteres

# Configuración del servidor
NODE_ENV=development
PORT=3000
CORS_ORIGIN=http://localhost:3000
